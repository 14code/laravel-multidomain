{
    "name": "gecche/laravel-multidomain",
    "description": "Laravel App on a multidomain setting",
    "keywords": ["laravel", "domain", "multidomain"],
    "license": "MIT",
    "authors": [
        {
            "name": "Giacomo Terreni",
            "email": "giacomo.terreni@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": ">=5.1.0"
    },
    "autoload": {
        "psr-4": {
            "Gecche\\Multidomain\\": "src/"
        },
        "files": [
            "src/Foundation/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gecche\\Multidomain\\Foundation\\DomainConsoleServiceProvider"
            ]
        }
    }
}
